{
  "name": "prevideo-downloader",
  "version": "1.0.0",
  "description": "YouTube video downloader with bilingual subtitle support and compression",
  "main": "src/main/main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "NODE_ENV=development electron .",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer && npm run build:electron",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build:electron": "electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:all": "npm run build && electron-builder -mw",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "download-binaries": "node scripts/download-binaries.js",
    "update-ytdlp": "node scripts/update-ytdlp.js",
    "download-whisper-models": "node scripts/download-whisper-models.js",
    "analyze": "source-map-explorer dist/**/*.js",
    "debug:main": "node --inspect=9229 ./node_modules/.bin/electron .",
    "debug:renderer": "vite --debug"
  },
  "keywords": [
    "youtube",
    "downloader",
    "subtitle",
    "electron",
    "ffmpeg",
    "whisper"
  ],
  "author": "PreVideo Inc.",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "electron": "^38.1.2",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "playwright": "^1.40.1",
    "prettier": "^3.1.1",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.4.4",
    "tsc-watch": "^6.3.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@reduxjs/toolkit": "^2.9.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "fluent-ffmpeg": "^2.1.2",
    "i18next": "^23.7.11",
    "notistack": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.21.1"
  },
  "build": {
    "appId": "com.prevideo.downloader",
    "productName": "PreVideo Downloader",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "resources/**/*",
      "!**/*.map"
    ],
    "extraResources": [
      {
        "from": "resources/binaries",
        "to": "binaries",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "language": "2052"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.icns",
      "category": "public.app-category.video",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons"
    }
  }
}
